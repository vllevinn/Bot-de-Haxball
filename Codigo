        room.sendAnnouncement(" ‚ÄØ‚ÄØ‚ÄØ‚ÄØ‚ÄØ ‚ÄØ‚ÄØ‚ÄØ‚ÄØ‚ÄØ ‚ÄØ‚ÄØ‚ÄØ‚ÄØ‚ÄØ ‚ÄØ‚ÄØ‚ÄØ‚ÄØ‚ÄØ ‚ÄØ‚ÄØ‚ÄØ‚ÄØ‚ÄØ‚ÄØ‚ÄØ‚ÄØ ‚ÄØ‚ÄØ‚ÄØ‚ÄØ‚ÄØ‚ÄØüí¨ Discord Link: ‚û° https://discord.gg/ATd63bS9He ‚¨Ö", null, 0xF6FF43, 'bold');
    }
 

    if (link.includes(message[0])) {
        room.sendAnnouncement("No podes enviar links ac√°. No entendes que es una dictadura esto?  " + player.name, player.id, Cor.Warn, "italic", 2);
        return false;
    }

    if (link.includes(message[1])) {
        room.sendAnnouncement("No podes enviar links ac√°. No entendes que es una dictadura esto? " + player.name, player.id, Cor.Warn, "italic", 2);
        return false;
    }

    if (link.includes(message[2])) {
        room.sendAnnouncement("No podes enviar links ac√°. No entendes que es una dictadura esto? " + player.name, player.id, Cor.Warn, "italic", 2);
